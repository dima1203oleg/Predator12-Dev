{
  "dashboard": {
    "title": "Predator11 Performance",
    "panels": [
      {
        "title": "Query Latency",
        "type": "timeseries",
        "metrics": [
          {
            "name": "avg_latency",
            "query": "avg(opensearch_query_latency)"
          }
        ]
      },
      {
        "title": "Cache Effectiveness",
        "type": "metric",
        "metrics": [
          {
            "name": "hit_rate",
            "query": "cache_hits_total / (cache_hits_total + cache_misses_total)"
          }
        ]
      }
    ],
    "alerts": [
      {
        "name": "high_latency",
        "condition": "opensearch_query_latency > 1000",
        "severity": "warning"
      }
    ]
  }
}
