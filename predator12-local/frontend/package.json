    "framer-motion": "^10.18.0",
    "howler": "^2.2.4",
    "i18next": "^25.5.3",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
